--collisions
function collide(a1)
	for a2 in all(actors) do
		if a2 != a1 then
			d=sqrt(((a1.x+4)-(a2.x+4))^2
				+((a1.y+4)-(a2.y+4))^2)
			
			if d<6 then
				return a2
			end
		end
	end
	
	return nil
end
