--player

function movement(p)
	if btn(⬆️) then
		p.dy=-1
		p.s=2
		if p.y<8 then
			p.dy=0
		end
	end
	
	if btn(⬇️) then
		p.dy=1
		p.s=3
		if p.y>114 then
			p.dy=0
		end
	end
	
	if btn(⬅️) then
		p.dx=-1.5
		if p.x<=0 then
			p.dx=0
		end
	end
	
	if btn(➡️) then
		p.dx=1
		if p.x>=128 then
			p.dx=0
		end
	end
	
	if not btn(⬅️) and 
		not btn(➡️) then
	 p.dx=-.5
	 if p.x<=0 then
			p.dx=0
		end
	end
	
	
	if not btn(⬆️) and 
		not btn(⬇️) then
	 p.dy=0
	 p.s=1
	end
	
	if btnp(❎) then
		make_bull()
		sfx(0)
	end
	
	if p.dying then 
		p.dx=-.2
		p.dy=.4
		
		if rnd(10)<2 then
			ex_smoke()
		end
		
		if p.y>116 then
			state="end"
			p.dead=true end
		
	end
	
end
