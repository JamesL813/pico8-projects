--microchip
--james

function _init()
	t=0
	score=0
	
	actors={}
	parts={}
	
	state="start"
	
	p=make_actor(1,32,64)
	make_actor(4,24,64)
	
	test = 0
	
end

function _update60()
	if state=="start" then
		update_start()
	elseif state=="game" then
		update_game()
	elseif state=="end" then
		update_end()
	end
end

function _draw()

	

	if state=="start" then
		draw_start()
	elseif state=="game" then
		draw_game()
	elseif state=="end" then
		draw_end()
	end
	
	--sspr(12*8,32,64,64,0,0,256,256)
	
end

--states
function update_start()
	if btn(❎) then
		state="game"
	end
end

function update_game()
	t+=1

	if btn(➡️) then
		sfx(2,1) else	sfx(1,1) end

	init_part(1)	

	if rnd(100)<90 then 
		init_part(0)
	end
	
	r=200-t*.05
	
	if r < 20 then
		r=20
	end
	
	if rnd(r)<1 then
		make_vir()
	end
	
	for a in all(actors) do
		update_actor(a)
	
		a2=collide(a)
	
		if a2!=nil then
			if a.k==1 and a2.k==3 then
				a.dying=true
				a.timer=0
			end
			--bullet collides with virus
			if a.k==3 and a2.k==2 then
				a.dead=true
				a2.dead=true
				score+=1
				sfx(4)
				expl_part(a2.x,a2.y)
			end
		end
	end
	
end

function update_end()
	if btnp(❎) then
		state="start"
		_init()
	end
end

function draw_start()
	cls(8)
	
	print("press ❎ to start!",
		31,31,2)
	
	print("press ❎ to start!",
		32,32,7)
	
end

function draw_game()
	cls(2)

	rectfill(0,0,127,8,8)
	rectfill(0,120,127,127,8)
	

	
	for i=0,176 do
	--rectfill(0,0,127,1,14)
	--rectfill(0,120,127,121,14)
		y=t*1.5
		for j=0,3 do
			pset(i-t%45,
				8+j*cos(i/45),2)
			
			pset(i-t%45,
				120-(.2+j*cos(i/45)),2)
		end
		

	
	end

	local r=rnd(10)
	
	--draw particles
	for p in all(parts) do
		update_part(p)
		
		if p.k==0 then
			circfill(p.x,p.y,p.r,p.col)
		elseif p.k==1 then
		
		circfill(p.x+(p.r*.5),p.y+(p.r*.5),
			p.r-(p.r*.1),p.col-1)
		
		
		
		circfill(p.x,p.y,p.r,p.col)
		elseif p.k==3 then
			circfill(p.x,p.y,p.r,5+rnd(5))
		end
	end
	
	--draw actors
	for a in all(actors) do
		spr(a.s,a.x,a.y)
		--if btn(➡️) then
			--spr(34,p.x-8,p.y)
		--else
			--spr(18,p.x-8,p.y)
		--end
	end
	
	if t<80 then
		init_part(2)
		rectfill(0,56,80-t,80,13)
		rectfill(0,75,80-t,80,1)
	end
	
	--print(test,23,67,8)
	
	print("score: "..score,0,0,2)
	print("score: "..score,1,1,7)
end

function draw_end()
	cls(2)
	print("game over",47,35,8)
	print("game over",48,36,7)
	
	
	print("score: "..score,47,51,8)
	print("score: "..score,48,52,7)
	
	
	print("press ❎ to try again!"
		,23,67,8)
		print("press ❎ to try again!"
		,24,68,7)

end



