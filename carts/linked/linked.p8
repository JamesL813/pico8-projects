--bubbles

function init_bub()
	col=7
	
	if rnd(2)<1 then col=12 end
	
	bub={
		x=p.x,
		y=p.y,
		col=col,
		r=rnd(3),
		dx=0,
		dy=-1,
		t=0,
	}
	
	add(bubs,bub)
end

function update_bub(b)
	b.dx=cos(b.t/40)
	b.t+=1
	
	b.x+=b.dx
	b.y+=b.dy
end

function draw_bub(b)
	circ(b.x,b.y,b.r,b.col)
end
