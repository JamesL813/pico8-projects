--scan

function scan_map()
	for y=0,128,8 do
		for x=0,128,8 do
		
			local s=mget(x/8,y/8)
			local f=fget(s,0)
			local sol=fget(s,1)
			
			local lnk=false
			if s==5 then lnk=true end
			
			if f then
				add(tiles,{
					sp=s,x=x,y=y,sol=sol,
					lnk=lnk,obj=nil})
			end
			
			if s==16 then
				key.x=x key.y=y
			end
			
		end
	end
end
