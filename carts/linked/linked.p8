--linked

function _init()
	t=0
	
	l=0
	
	start_lvl(l)
	
end

function start_lvl(l)
	max_dist=40
	
	p={
		x=8,y=8,
		w=8,h=8,
		dx=0,dy=0,flp=false,
		win=false}
	
	anch={
		sp=1,
		x=32,y=64,
		w=8,h=8,
		dx=0,
		dy=0,
		spd=.4
		}
	
	key={x=-1,y=-1}
	
	chns={}
	numc=max_dist/10
	
	bubs={}
	
	text={on=false,x=0,y=0,txt=""}
	ttext={on=false,x=0,y=0,txt=""}
	
	
	tiles={}
	scan_map()
	
	objs={3,9}
	local i=1
	for t in all(tiles) do
		if t.sp==5 then 
			t.obj=objs[i] i+=1 end
	end
	
	r={x1=0,x2=0,y1=0,y2=0}
end

function _update60()
	t+=1

	--debug
	test=false

	dist=sqrt((p.x-anch.x)^2
		+(p.y-anch.y)^2)
	
	--anchor unit vector
	av={
		x=(anch.x-p.x)/dist,
		y=(anch.y-p.y)/dist
	}
	
	if rnd(20)<1 then
		init_bub()
	end
	
	makechain(numc)
	
	movement()
	interact()
	
	for b in all(bubs) do
		update_bub(b)
	end
	
	anch.x+=anch.dx
	anch.y+=anch.dy
	
	
end

function _draw()
	cls(1)
	map(l*16,0)
	
	
	for c in all(chns) do
		circ(c.x,c.y,2,6)
		del(chns,c)
	end
	spr(1,anch.x,anch.y)
	
	if t%30>15 then
		spr(32,p.x,p.y,1,1,p.flp)
	else
		spr(33,p.x,p.y,1,1,p.flp)
	end
	
	if text.on then 
		print(text.txt,
			text.x,text.y,0) 
		print(text.txt,
			text.x+1,text.y+1,7)
	end
	
	if t<200 then
		print("get the key!",-32+t,32,0)
		print("get the key!",-33+t,33,7)
		
	end
	
	for b in all(bubs) do
		draw_bub(b)
	end
	
	if collideobj(p,key,"all") then
		
		print("level complete",34,64,0)
		print("level complete",35,65,7)
		
	end
	
	--debug
	--print("dist: "..dist,2,2)
	--print(ttext.txt,8,8)
	--rect(r.x1,r.y1,r.x2,r.y2)
	--print(test,p.x,p.y-10)
end
