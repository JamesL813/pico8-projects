--collisions

function collideobj(o1,o2,dir)
	
	local dist_x=o1.x-o2.x
	local dist_y=o1.y-o2.y
	
	if dir=="left" then
		if dist_x>0 and dist_x<4
			and dist_y<4 and dist_y>-4 then
			return true
		else return false end
		
	elseif dir=="right" then
		if dist_x<0 and dist_x>-4
			and dist_y<4 and dist_y>-4 then
			return true
		else return false end
		
	elseif dir=="up" then
		if dist_x<4 and dist_x>-4
			and dist_y<4 and dist_y>0 then
			return true
		else return false end
		
	elseif dir=="down" then
		if dist_x<4 and dist_x>-4
			and dist_y>-4 and dist_y<0 then
			return true
		else return false end
		
	elseif dir=="all" then
		if dist_x<4 and dist_x>-4
			and dist_y<4 and dist_y>-4 then
			return true
		else return false end
	end
end

function collide_map(a,dr,flg)
	
	local x=a.x local y=a.y
	local w=a.w local h=a.h
	
	local x1=0 local y1=0
	local x2=0 local y2=0
	
	if dr=="left" then
		x1=x-2		y1=y+2
		x2=x-2		y2=y+h-1
		
		
	elseif dr=="right" then
		x1=x+w		y1=y+2
		x2=x+w	y2=y+h-1
	elseif dr=="up" then
		x1=x+1		y1=y-1
		x2=x+w-2	y2=y
		
		
	elseif dr=="down" then
		x1=x+1			y1=y+h
		x2=x+w-2		y2=y+h
	end
	
	--debug
		r.x1=x1 r.y1=y1
		r.x2=x2 r.y2=y2
	
	--pixels to tiles
	x1/=8		y1/=8
	x2/=8		y2/=8
	
	if fget(mget(x1,y1), flg)
	or fget(mget(x1,y2), flg)
	or fget(mget(x2,y1), flg)
	or fget(mget(x2,y2), flg)
	then
	
		return true
	else
		return false
	end
end
