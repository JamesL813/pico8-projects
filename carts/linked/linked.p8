--chain system

function makechain(n)
	xi=-(p.x-anch.x)/(n+1)
	yi=-(p.y-anch.y)/(n+1)
	
	for i=1,n do
		add(chns,
			{x=p.x+xi*i+4,y=p.y+yi*i+4})
	end
end
