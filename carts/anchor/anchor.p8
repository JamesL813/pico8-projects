--movement

function movement()
	local dx=0
	local dy=0
	
	if btn(⬅️) then
		dx=-1 
		if collideobj(p,anch,
			"left") then
			moveanch("left")
			dx=0
		end
		
		if collide_map(p,"left",2)
			then dx=0 dy=0 
			test=true
		end
		p.flp=true
	end
	if btn(➡️) then
	 dx=1
	 if collideobj(p,anch,
			"right") then
			moveanch("right")
			dx=0
		end
		
		if collide_map(p,"right",2)
			then dx=0 dy=0 
			test=true
		
		end
		
		p.flp=false
	end
	if btn(⬆️) then
	 dy=-1
	 if collideobj(p,anch,
			"up") then
			moveanch("up")
			dy=0
		end
		
		if collide_map(p,"up",2)
			then dx=0 dy=0 
			test=true 
		
		end
	end
	if btn(⬇️) then
		dy=1
		if collideobj(p,anch,
			"down") then
			moveanch("down")
			dy=0
		end
		
		if collide_map(p,"down",2)
			then dx=0 dy=0 
			test=true 
		end
	end
	
	--magnitude of movement vector
	m=sqrt(dx^2+dy^2)
	
	if m<1.4 then
		p.dx=dx
		p.dy=dy
	else
		p.dx=dx/m
		p.dy=dy/m
	end
	
	if dist>max_dist then
		
		if t%10==1 then
			sfx(0)
		end
		p.x+=(dist/40)*av.x
		p.y+=(dist/40)*av.y
	else
	
		p.x+=p.dx
		p.y+=p.dy
	end
end

function moveanch(dir)
	
	
	if dir=="left" then
		local canmv=true
		for t in all(tiles) do
			if collideobj(anch,t,"left")
				and fget(t.sp,1)
				then canmv=false end
		end
		
		if canmv then 
			anch.x-=anch.spd end
		
	elseif dir=="right" then
		local canmv=true
		for t in all(tiles) do
			if collideobj(anch,t,"right")
				and fget(t.sp,1)
				then canmv=false end
		end
		
		if canmv then
			anch.x+=anch.spd end
		
	elseif dir=="up" then
		local canmv=true
		for t in all(tiles) do
			if collideobj(anch,t,"up")
				and fget(t.sp,1)
				then canmv=false end
		end
		
		if canmv then
			anch.y-=anch.spd end
			
	elseif dir=="down" then
		local canmv=true
		for t in all(tiles) do
			if collideobj(anch,t,"down")
				and fget(t.sp,1)
				then canmv=false end
		end
		
		if canmv then
			anch.y+=anch.spd end
	end 
end
