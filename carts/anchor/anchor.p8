--states

function update_game()
		t+=1

	if rnd(200)<1 then
		add(fish,
			{x=128*(l+1),y=rnd(128)})
	end
	
	--debug
	test=false

	numc=max_dist/10
	
	dist=sqrt((p.x-anch.x)^2
		+(p.y-anch.y)^2)
	
	--anchor unit vector
	av={
		x=(anch.x-p.x)/dist,
		y=(anch.y-p.y)/dist
	}
	
	if rnd(20)<1 then
		init_bub()
	end
	
	makechain(numc)
	
	movement()
	interact()
	
	for f in all(fish) do
		f.x-=1
		f.y+=sin(t/50)
	end
	
	for b in all(bubs) do
		update_bub(b)
	end
	
	anch.x+=anch.dx
	anch.y+=anch.dy
	camera(l*128,0)
end

function draw_game()
	cls(1)
	map(0,0)
	
	
	for c in all(chns) do
		circ(c.x,c.y,2,6)
		del(chns,c)
	end
	if rings[l+1].x!=nil then
		circ(rings[l+1].x,
			(rings[l+1].y)+3*sin(t/30),
				2,5+((t/30)%2))
		if distance(p.x+4,p.y+4,
			rings[l+1].x,rings[l+1].y)<4 then
			max_dist+=10
			rings[l+1]={}
			sfx(2)
		end
	end
	
	spr(1,anch.x,anch.y)
	
	if t%30>15 then
		spr(32,p.x,p.y,1,1,p.flp)
	else
		spr(33,p.x,p.y,1,1,p.flp)
	end
	
	if text.on then 
		print(text.txt,
			text.x,text.y,0) 
		print(text.txt,
			text.x+1,text.y+1,7)
	end
	
	if t<350 then
		print("get the key!",
			-32+(t/2)+128*l,64+3*sin(t/30),0)
		print("get the key!",
			-33+(t/2)+128*l,65+3*sin(t/30),7)
		
	end
	
	for b in all(bubs) do
		draw_bub(b)
	end
	
	for f in all(fish) do
		spr(18+(t/5)%2,f.x,f.y)
	end
	
	if collideobj(p,key,"all") then
		
		lvls[l+1]=true
		
		init_menu()
		state="menu"
		l=0
		camera(0,0)
		
		--print("level complete",34+(128*l),64,0)
		--print("level complete",35+128*l,65,7)
		
	end
end

function init_menu()
	cur=0
	at=0
end

function update_menu()
	t+=1
	

	
	if btnp(➡️) then cur+=1 end
	if btnp(⬅️) then cur-=1 end
	cur=mid(0,cur,1)
	
	if btnp(❎) then
		l=cur
		state="game"
		start_lvl(cur) end
	
end

function draw_menu()
	cls(1)
	for i=0,1 do
		if lvls[i+1] then
			rectfill(24+i*20,35,38+i*20,52,5)
			rectfill(26+i*20,38,36+i*20,48,11)
			print(i+1,30+i*20,41,7)
		else
			rectfill(24+i*20,35,38+i*20,52,5)
	
			rectfill(26+i*20,32,36+i*20,48,3)
			rectfill(26+i*20,32,36+i*20,42,11)
		
			print(i+1,30+i*20,35,7)
		end
	end
	rect(23+cur*20,31,39+cur*20,53,7)
	
	spr(64,48,70+5*sin(t/100),4,4)
	
	if lvls[1]==true 
		and lvls[2]==true then
		print("you win!",52,16,0)
		print("you win!",53,17,7)
		
	end
		
end
