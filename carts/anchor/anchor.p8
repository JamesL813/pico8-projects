--intertaction

function interact()
	if (collide_map(p,"up",3) 
		or collide_map(p,"down",3)
		or collide_map(p,"left",3)
		or collide_map(p,"right",3))
		then
	text.on=true
	text.x=p.x-12 text.y=p.y-10
	text.txt="press ❎"
	
	local x=p.x/8
	local y=p.y/8
	
	local tx=0 local ty=0
	local obj=-1
	
	
	
	if btn(❎) then
		for t in all(tiles) do
			if t.sp==5 and 
				distance(p.x,p.y,t.x,t.y)<8 then
				tx=t.x
				ty=t.y
				obj=t.obj
				t.sp+=1
				mset(tx/8,ty/8,t.sp)
			end
		end
		
		
		ttext.on=true
		ttext.x=p.x
		ttext.y=p.y
		ttext.txt=obj
		
		for t in all(tiles) do
			if t.x==tx and t.y==ty then
				for t2 in all(tiles) do
					if t2.sp==t.obj then
						t2.sp+=1
						mset(t2.x/8,t2.y/8,t2.sp)
						sfx(1)
					end
				end
			end
		end
		
	end
		
	else
	text.on=false
	end
		
end

function distance(x1,y1,x2,y2)
  return sqrt((x2-x1)^2+
  	(y2-y1)^2)
end
