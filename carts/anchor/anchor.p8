function _init()
	state="menu"
	
	t=0
	
	l=0
	lvls={false,false,false,false}
	
	p_loc={{x=64,y=4},
		{x=136,y=8}}
	a_loc={{x=64,y=16},
		{x=160,y=64}}
		
	rings={{x=16,y=16},
		{x=216,y=16}}
	
	init_menu()
end

function start_lvl(l)
	max_dist=40
	
	p={
		x=p_loc[l+1].x,
		y=p_loc[l+1].y,
		w=8,h=8,
		dx=0,dy=0,flp=false,
		win=false}
	
	anch={
		sp=1,
		x=a_loc[l+1].x,
		y=a_loc[l+1].y,
		w=8,h=8,
		dx=0,
		dy=0,
		spd=.4
		}
	
	key={x=-1,y=-1}
	
	fish={}
	chns={}
	
	numc=max_dist/10
	
	bubs={}
	
	text={on=false,x=0,y=0,txt=""}
	ttext={on=false,x=0,y=0,txt=""}
	
	
	tiles={}
	scan_map(l)
	
	objs={3,9}
	local i=1
	for t in all(tiles) do
		if t.sp==5 then 
			t.obj=objs[i] i+=1 end
	end
	
	r={x1=0,x2=0,y1=0,y2=0}
end

function _update60()
	if state=="game" then
		update_game()
	elseif state=="menu" then
		update_menu()
	end
end

function _draw()

	if state=="game" then
		draw_game()
		
	elseif state=="menu" then
		draw_menu()
	end
	
	--print(lvls[2],12,12,0)
	
	--debug
	--print(key.x..", "..key.y,132,2)
	--print(ttext.txt,8,8)
	--rect(r.x1,r.y1,r.x2,r.y2)
	
end
