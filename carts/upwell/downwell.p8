--initialization

--runs at start
function _init()
	p={
		sp=1,
		x=16,    y=32,
		w=8,     h=8,
		b=3,
		flp=false,
		dx=0,    dy=0,
		max_dx=1.5,max_dy=4,
		acc=0.3,
		jump=3.5, --jump boost
		anim=0,
		maxammo=5,
		ammo=5,
		bl_timer=0,
		running=false,
		jumping=false,
		shooting=false,
		falling=false,
		wsliding=true, --wall sliding
		w_cd=0,
		bsliding=false, -- boost sliding
		run_anim={2,1,3,1},
		run_i=1
	}
	bullets={}
	enemies={}
	motln={} --motion lines
	
	grav=0.16
	fric=0.85
	
	--simple camera
	cam_x=0
	cam_y=0
	
	--map limits
	map_start_x=0
	map_end_x=128
	map_start_y=0
	map_end_y=384

	
	--test code-----------
	x1r=0 y1r=0 x2r=0 y2r=0
	collide_l="no"
	collide_r="no"
	collide_u="no"
	collide_d="no"
	
	en_init(64, 64, "bat")
	en_init(32, 64, "bat")
	en_init(64, 32, "slime")
	en_init(32, 32, "slime")
	
	
	----------------------
end
