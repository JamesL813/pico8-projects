--player

function p_update()
	--physics
	if p.dy <= p.max_dy then
		p.dy+=grav
	else
		p.dy=p.max_dy
	end
	p.dx*=fric
	
	--controls
	
	--running
	if btn(⬅️) and not p.bsliding then
		p.dx-=p.acc
		if not p.running 
		and not p.jumping
		and not p.falling then
			ml_init(p.x+p.w,p.y,true)
			sfx(0)
		end
		p.running=true
		p.flp=true
	end
	if btn(➡️) and 
		not p.bsliding then
		p.dx+=p.acc
		if not p.running 
		and not p.jumping
		and not p.falling then
			ml_init(p.x-p.w,p.y,false)
			sfx(0)
		end
		p.running=true
		p.flp=false
	end
	
	--boost sliding
	if (btn(⬅️) or btn(➡️)) 
		and btn(⬇️) and not p.jumping 
		and not p.falling then
		if (p.bsliding==false) then
			if p.flp then
				p.dx-=p.b
			else
				p.dx+=p.b
			end
		end
		p.bsliding = true
		p.running = false
	end
	
	if not btn(⬇️) then
		p.bsliding = false
	end
	
	--slide
	if p.running 
	and not btn(⬅️)
	and not btn(➡️)
	and not p.falling
	and not p.jumping then
		p.running=false
	--	p.sliding=true
	end
	
	--jump
	if (btnp(🅾️) or btnp(❎)) then
		if p.landed then
			p.dy-=p.jump+(abs(p.dx)*.25)
			ml_init(p.x+p.w,p.y,true)
			ml_init(p.x-p.w,p.y,false)
			sfx(1)
			p.landed=false
		--wall jump
		elseif p.wsliding then
			p.dy=0
			p.dy-=p.jump+(abs(p.dx)*.25)
			ml_init(p.x,p.y,p.flp)
			--ml_init(p.x-p.w,p.y,false)
			sfx(1)
			p.landed=false
			p.wsliding=false
			p.w_cd=1
		
		else
			--shooting
			if (p.jumping or p.falling) 
				and (btnp(❎) or btnp(🅾️))
				and p.ammo>0 
				and p.bl_timer==0 then
					if p.jumping then
						p.dy-=.95
					else
						p.dy-=abs(p.dy)
					end
					p.ammo-=1
					p.bl_timer=12
					p.sp=4
					sfx(2)
					bl_init(p.x,p.y+4)
				end
			end
		end
	--shoot
	
	
	--reset bullet cooldown
	if p.bl_timer > 0 then
		p.bl_timer-=1
	end
	
	if collide_map(p,"up",2) or
	 collide_map(p,"down",2) or
	 collide_map(p,"left",2) or
	 collide_map(p,"right",2) then
			--p.dy-=4
			--test--------
			collide_u="yes"
		else
			collide_u="no"	
			--------------
		end
	
	--check y collision
	if p.dy>0 then
		--if falling
		
		p.falling=true
		p.landed=false
		p.jumping=false
		p.sliding=false
		
		--landing
		if collide_map(p,"down",0) then
			if p.ammo<p.maxammo then
				sfx(4)
			end
			p.w_cd=0
			p.ammo=p.maxammo
			--sfx(3)
			p.landed=true
			
			p.falling=false
			p.dy=0
			p.y-=((p.y+p.h+1)%8)-1
		end
		
	elseif p.dy<0 then
		--if rising
		p.jumping=true
		if collide_map(p,"up",1) then
			p.dy=0 --set y momentum to 0
		end
 end
 
	--check left and right collision
	if p.dx<0 then
		if collide_map(p,"left",1) then
			p.dx=0
		end
	elseif p.dx>0 then
		if collide_map(p,"right",1) then
			p.dx=0
			--test--------
			collide_r="yes"
		else
			collide_r="no"	
			--------------
		end
	end
	
	--wall jump
	if (collide_map(p,"left",1) 
		or collide_map(p,"right",1)) 
		and p.falling and p.w_cd==0 then
		p.wsliding=true
		--p.dy=0
	--test--------
		collide_l="yes"
	else
		collide_l="no"	
			--------------	
			p.wsliding=false	
	end
	
	
	p.x+=p.dx
	p.y+=p.dy
	
	--limit player to map
	if p.x<0 then
		p.x=0
	elseif p.x>map_end_x-p.w then
		p.x=map_end_x-p.w
	end
	
end

function p_draw()
	if p.jumping then
		if abs(p.dy) <1 then
			p.sp=4
		else
			p.sp=6
		end
	elseif p.falling then
		if abs(p.dy) <1 then
			p.sp=4
		else
			p.sp=5
		end
	elseif p.sliding then
		p.sp=7
	elseif p.running then
		if time()-p.anim>.1 then
		p.anim=time()
		p.sp=p.run_anim[p.run_i]
		p.run_i+=1
			if p.run_i >4 then
				p.run_i=1
			end
		end
	elseif p.wsliding then
		p.sp=8
	else
	--idle
		p.sp=1
	end
	
	if p.bsliding then
		p.sp=7
	end
	
end


function limit_speed(num,maximum)
	return mid(-maximum,num,maximum)
end


