--update and draw

--runs every frame
function _update60()
	p_update()
	p_draw()
	
	--simple camera
	cam_x=0--p.x-64+(p.w/2)
	cam_y=p.y-64+(p.h/2)
	if cam_x<map_start_x then
		cam_x=map_start_x
	end
	if cam_x>map_end_x-128 then
		cam_x=map_end_x
 end
	camera(cam_x,cam_y)
	
	
	
	
end

--runs every frame
function _draw()
	cls()
	map(0,0,0,0,16,64)
	spr(p.sp,p.x,p.y,1,1,p.flp)
	--updtes effects
	for ml in all (motln) do
		ml_update(ml)
		spr(ml.sp, ml.x,ml.y,1,1,ml.flp)
	end
	
	--upadtes bullets
	for bl in all (bullets) do
		bl_update(bl)
		spr(bl.sp, bl.x,bl.y,1,1,false)
	end
	
	--updtes enemies
	for en in all (enemies) do
		en_update(en)
		spr(en.sp, en.x,en.y,1,1,en.flp)
	end
	
	
	
	--test code-------
	--rect(x1r,y1r,x2r,y2r,6)
	print("⬅️="..collide_l,p.x,p.y-10)
	print("⬆️="..collide_u,p.x,p.y-16)
	--print("➡️="..collide_r,p.x,p.y-16)
	--print("⬇️="..collide_d,p.x,p.y-22)
	print("w="..tostr(p.wsliding),cam_x,cam_y)
	print("w_cd="..tostr(p.w_cd),cam_x,cam_y+8)
	
	------------------
end
