--collisions

function collide_map(obj,aim,flag)
	--obj = table needs x,y,w,h
	--aim = left,right,up,down
	
	local x=obj.x
	local y=obj.y
	local w=obj.w
	local h=obj.h
	
	local x1=0
	local y1=0
	local x2=0
	local y2=0
	
	if aim=="left" then
		x1=x-1
		y1=y+1
		x2=x
		y2=y+h-1
		
	elseif aim=="right" then
		x1=x+w-1
		y1=y+1
		x2=x+w
		y2=y+h-1
		
	elseif aim=="up" then
		x1=x+1
		y1=y-1
		x2=x+w-3
		y2=y
	elseif aim=="down" then
		x1=x+1
		y1=y+h
		x2=x+w-3
		y2=y+h+1
	end
	
	--test----
	x1r=x1 x2r=x2 y1r=y1 y2r=y2
	----------
	
	
	--pixels to tiles
	x1/=8
	y1/=8
	x2/=8
	y2/=8
	
	--checks if collided with flag
	if fget(mget(x1,y1),flag)
	or fget(mget(x1,y2),flag)
	or fget(mget(x2,y1),flag)
	or fget(mget(x2,y2),flag) then
		return true
	else
		return false
	end
	
end

function collide_enemy(obj,flag)
end
