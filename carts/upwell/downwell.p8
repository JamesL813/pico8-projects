--enemies and effects

function ml_init(x,y,flp)
		add(motln,{
		sp=11,
		x=x,	y=y,
		flp=flp	})
end

function bl_init(x,y)
		add(bullets,{
		sp=48,
		x=x,	y=y})
end

function en_init(x,y,typ)
		if typ == "bat" then
			sprite=18
		elseif typ == "slime" then
			sprite=16
		end
		add(enemies,{
		sp=sprite,
		x=x,	y=y,
		dx=.05,dy=.03, 
		flp=false,
		typ=typ})
end

--animate action lines
function ml_update(ml)
		if ml.sp >= 11 and ml.sp < 15 then
			ml.sp+=.5
		else
			ml.sp=255
			del{motln, ml}
		end
end

--bullets
function bl_update(bl)

	for e in all(enemies) do
		if abs(e.x-bl.x)<4 
			and abs(e.y-bl.y)<4 then
		
			del(enemies,e)
			del(bullets,bl)
			
			sfx(5)
			
			return
		
		end
	end

	if bl.sp >= 48 
	and bl.sp < 50 then
			if bl.sp >= 48 
			and bl.sp < 49 then
			bl.sp+=.075
			else
			bl.sp+=1
			end
			bl.y+=3
		else
			bl.sp=255
			del{bullets, bl}
		end
end

--animate enemies
function en_update(en)
		
		if (abs(en.x-p.x)<4
			and abs(en.y-p.y)<2)
		 then
				
				del(enemies,en)
				
				p.dy=-3
				sfx(5)
				
				return
		
		end
		
		mult= rnd(15)
		
		if en.x < p.x then
			en.x+=en.dx*mult
			en.flp=true
		else
			en.x-=en.dx*mult
			en.flp=false
		end
		
		mult= rnd(30)
		
		if en.y < p.y then
			en.y+=en.dy*mult
			else
			en.y-=en.dy*mult
		end
		
		if en.typ == "bat" then
			if en.sp < 20 then
				en.sp+=.1
			else
				en.sp=18
			end
		end
		
		if en.typ == "slime" then
			if time() % 1 < .7 then
				en.sp=16
			else
				en.sp=17
			end
		end
end
