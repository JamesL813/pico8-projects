pico-8 cartridge // http://www.pico-8.com
version 36
__lua__

function _init()
	t=0
	b={x=60,y=60,w=7,h=7}

end

function _update60()

	t+=1
	
	b.y+=sin(t/100)/10

end

function _draw()
	cls()
	rectfill(b.x,b.y,
		b.x+b.w,b.y+b.h, 7)
	
	dicenum(b,1)
	

end


function dicenum(_b,n)
	pset(_b.x+(_b.w/2),flr(_b.y)+(_b.h/2),3)
end
